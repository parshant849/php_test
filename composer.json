{
    "name": "laravel/lumen",
    "description": "The Laravel Lumen Framework.",
    "keywords": [
        "framework",
        "laravel",
        "lumen"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "barryvdh/laravel-cors": "^0.11.0",
        "darkaonline/swagger-lume": "^5.6",
        "doctrine/dbal": "^2.6",
        "flipbox/lumen-generator": "^5.6",
        "guzzlehttp/guzzle": "^6.3",
        "illuminate/redis": "^5.6",
        "illuminate/session": "^5.6",
        "laravel/lumen-framework": "5.6.*",
        "league/flysystem-aws-s3-v3": "~1.0",
        "sammyk/laravel-facebook-sdk": "^3.5",
        "sentry/sentry-laravel": "^0.9.2",
        "thujohn/twitter": "^2.2",
        "vlucas/phpdotenv": "~2.2",
        "wronx/lumen-rest-object-fetch-middleware": "^0.1.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.4",
        "beyondcode/laravel-dump-server": "^1.1",
        "brainmaestro/composer-git-hooks": "^2.4",
        "codedungeon/phpunit-result-printer": "^0.11.2",
        "fzaninotto/faker": "~1.4",
        "jakub-onderka/php-parallel-lint": "^1.0",
        "mockery/mockery": "~1.0",
        "phpmd/phpmd": "^2.6",
        "phpunit/phpunit": "~7.0",
        "roave/security-advisories": "dev-master",
        "sensiolabs/security-checker": "^4.1",
        "squizlabs/php_codesniffer": "^3.2",
        "wronx/lumen-crud-generator": "^0.3.0",
        "google/apiclient": "2.0.*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/",
            "database/"
        ],
        "files": [
            "tests/helpers.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-install-cmd": [
            " if [ ! -f .env ]; then if [ -f .env.example ]; then cp .env.example .env; fi; fi",
            "[ -f vendor/bin/cghooks ] && vendor/bin/cghooks add --ignore-lock || echo 'cghooks not found'",
            "[ -d vendor/barryvdh/laravel-ide-helper ] && php artisan ide-helper:generate || echo 'IDE helper not found'",
            "[ -d vendor/barryvdh/laravel-ide-helper ] && php artisan ide-helper:meta || echo 'IDE helper not found'",
            "[ -d vendor/barryvdh/laravel-ide-helper ] && php artisan ide-helper:models || echo 'IDE helper not found'"
        ],
        "post-update-cmd": [
            "[ -f vendor/bin/cghooks ] && vendor/bin/cghooks update || echo 'cghooks not found'",
            "[ -d vendor/barryvdh/laravel-ide-helper ] && php artisan ide-helper:generate || echo 'IDE helper not found'",
            "[ -d vendor/barryvdh/laravel-ide-helper ] && php artisan ide-helper:meta || echo 'IDE helper not found'",
            "[ -d vendor/barryvdh/laravel-ide-helper ] && php artisan ide-helper:models || echo 'IDE helper not found'"
        ],
        "cghooks": "vendor/bin/cghooks"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "hooks": {
            "pre-commit": "chmod +x .deploy/pre_commit.sh && .deploy/pre_commit.sh"
        }
    }
}
